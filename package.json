  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "bundle": "rm -rf ./dist && bun lint && tsc -p tsconfig.build.json && tsc-alias && tailwindcss -i ./src/resume/global.css -o ./dist/resume/global.css",
